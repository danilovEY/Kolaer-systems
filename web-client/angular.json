{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1913,
  "newProjectRoot": "projects",
  "projects": {
    "kolaer-web-client": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/kolaer-web-client/1913",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/favicon.png",
              "src/assets"
            ],
            "styles": [
              "./node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "./node_modules/bootstrap/dist/css/bootstrap.css",
              "./node_modules/angular2-toaster/toaster.css",
              "./node_modules/typeface-exo/index.css",
              "./node_modules/ionicons/dist/scss/ionicons.scss",
              "./node_modules/socicon/css/socicon.css",
              "./node_modules/nebular-icons/scss/nebular-icons.scss",
              "./node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "./node_modules/@ng-select/ng-select/themes/default.theme.css",
              "./node_modules/primeicons/primeicons.css",
              "./node_modules/primeng/resources/themes/omega/theme.css",
              "./node_modules/primeng/resources/primeng.min.css",
              "./src/app/@theme/styles/styles.scss"
            ],
            "scripts": [
              "./node_modules/pace-js/pace.min.js",
              "./node_modules/tinymce/tinymce.min.js",
              "./node_modules/@fortawesome/fontawesome-free/js/all.js",
              "./node_modules/tinymce/themes/modern/theme.min.js",
              "./node_modules/tinymce/plugins/link/plugin.min.js",
              "./node_modules/tinymce/plugins/paste/plugin.min.js",
              "./node_modules/tinymce/plugins/table/plugin.min.js",
              "./node_modules/echarts/dist/echarts.min.js",
              "./node_modules/echarts/dist/extension/bmap.min.js",
              "./node_modules/chart.js/dist/Chart.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kolaer-web-client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "kolaer-web-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kolaer-web-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "kolaer-web-client-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "kolaer-web-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kolaer-web-client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "kolaer-web-client"
}
